issn = {17425468},
journal = {Journal of Statistical Mechanics: Theory and Experiment},
keywords = {driven diffusive systems (theory),exact results,extreme value statistics,stochastic particle dynamics (theory)},
number = {5},
title = {{Survival probability of an immobile target surrounded by mobile traps}},
volume = {2012},
year = {2012}
}
