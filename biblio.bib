journal = {Journal of Hydraulic Engineering},
keywords = {Entrainment,Error analysis,Probability,Sediment,Velocity distribution},
number = {4},
pages = {438--442},
title = {{Pickup probability of sediment under log-normal velocity distribution}},
volume = {128},
year = {2002}
