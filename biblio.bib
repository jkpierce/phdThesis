journal = {Gravel-Bed Rivers: Processes, Tools, Environments},
keywords = {Computational fluid dynamics,Discrete particle models,Exner equation},
pages = {116--122},
title = {{The Potential of Using High-Resolution Process Models to Inform Parameterizations of Morphodynamic Models}},
