journal = {Water Resources Research},
keywords = {Fluvial geomorphology,Gravel bed rivers,Sediment transport},
number = {10},
pages = {1--22},
pmid = {1201},
title = {{A general power equation for predicting bed load transport rates in gravel bed rivers}},
volume = {40},
year = {2004}
}
