doi = {10.1201/b17133-120},
file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Santos, Franca, Ferreira - 2014 - Coherent structures in open channel flows with bed load transport over an hydraulically rough bed.pdf:pdf},
isbn = {9781138026742},
pages = {883--890},
publisher = {Taylor {\&} Francis},
title = {{Coherent structures in open channel flows with bed load transport over an hydraulically rough bed}},
year = {2014}
