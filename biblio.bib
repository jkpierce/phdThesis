doi = {10.1016/j.ces.2018.05.034},
file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Seelen, Padding, Kuipers - 2018 - A granular Discrete Element Method for arbitrary convex particle shapes Method and packing generation.pdf:pdf},
issn = {00092509},
journal = {Chemical Engineering Science},
keywords = {Contact detection,Discrete element method,Non-spherical particle,Orientational ordering,Packing,Solid volume fraction},
pages = {84--101},
title = {{A granular Discrete Element Method for arbitrary convex particle shapes: Method and packing generation}},
