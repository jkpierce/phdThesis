journal = {Journal of Hydraulic Engineering},
keywords = {Bedload transport,Markov chains,Sediment interchange,Stochastic model},
number = {12},
pages = {1265--1276},
title = {{Modeling bed-load transport by a three-state continuous-time Markov Chain model}},
url = {http://ascelibrary.org/doi/10.1061/{\%}28ASCE{\%}29HY.1943-7900.0000764},
volume = {139},
