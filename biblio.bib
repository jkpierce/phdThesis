eprint = {arXiv:1011.1669v3},
file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/V{\"{o}}r{\"{o}}smarty et al. - 2010 - Global threats to human water security and river biodiversity.pdf:pdf},
isbn = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},
issn = {00280836},
journal = {Nature},
number = {7321},
pages = {334},
pmid = {20882010},
title = {{Global threats to human water security and river biodiversity}},
url = {http://www.nature.com/doifinder/10.1038/nature09549},
volume = {468},
year = {2010}
}
