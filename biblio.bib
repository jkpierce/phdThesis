@article{Kline1967,
abstract = {ABSTRACT Extensive visual and quantitative studies of turbulent boundary$\backslash$nlayers are described. Visual studies reveal the presence of surprisingly$\backslash$nwell-organized spatially and temporally dependent motions within$\backslash$nthe so-called {\^{a}}laminar sublayer{\^{a}}. These motions lead to the formation$\backslash$nof low-speed streaks in the region very near the wall. The streaks$\backslash$ninteract with the outer portions of the flow through a process of$\backslash$ngradual {\^{a}}lift-up{\^{a}}, then sudden oscillation, bursting, and ejection.$\backslash$nIt is felt that these processes play a dominant role in the production$\backslash$nof new turbulence and the transport of turbulence within the boundary$\backslash$nlayer on smooth walls.Quantitative data are presented providing an$\backslash$nassociation of the observed structure features with the accepted$\backslash$n{\^{a}}regions{\^{a}} of the boundary layer in non-dimensional co-ordinates;$\backslash$nthese data include zero, negative and positive pressure gradients$\backslash$non smooth walls. Instantaneous spanwise velocity profiles for the$\backslash$ninner layers are given, and dimensionless correlations for mean streak-spacing$\backslash$nand break-up frequency are presented.Tentative mechanisms for formation$\backslash$nand break-up of the low-speed streaks are proposed, and other evidence$\backslash$nregarding the implications and importance of the streak structure$\backslash$nin turbulent boundary layers is reviewed.},
author = {Kline, S. J. and Reynolds, W. C. and Schraub, F. A. and Runstadler, P. W.},
doi = {10.1017/S0022112067001740},
isbn = {1469-7645},
issn = {14697645},
journal = {Journal of Fluid Mechanics},
number = {4},
pages = {741--773},
title = {{The structure of turbulent boundary layers}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112067001740},
volume = {30},
year = {1967}
}
@article{VanRijn1984,
abstract = {A method is presented which enables the computation of the bedload transport as the product of the saltation height, the particle velocity and the bed-load concentration. The equations of motions for a solitary particle are solved numerically to determine the saltation height and particle velocity. Experiments with gravel particles (transported as bed load) are selected to calibrate the mathematical model using the lift coefficient as a free parameter. The model is used to compute the saltation heights and lengths for a range of flow conditions. The computational results are used to determine simple relationships for the saltation characteristics. Measured transport rates of the bed load are used to compute the sediment concentration in the bed-load layer. A simple expression specifying the bed-load concentration as a function of the flow and sediment conditions is proposed. A verification analysis using about 600 (alternative) data shows that about 77{\%} of the predicted bed load-transport rates are within 0.5 and 2 times the observed values. {\textcopyright} ASCE.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {van Rijn, Leo C.},
doi = {10.1061/(ASCE)0733-9429(1984)110:10(1431)},
eprint = {arXiv:1011.1669v3},
isbn = {0733-9429},
