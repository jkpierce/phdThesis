doi = {10.1109/CVPRW.2018.00190},
file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pedersen et al. - 2018 - Camera calibration for underwater 3D reconstruction based on ray tracing using snell's law.pdf:pdf},
isbn = {9781538661000},
