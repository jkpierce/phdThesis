journal = {Water Resources Research},
keywords = {doi:10.102,http://dx.doi.org/10.1029/2009WR008643},
number = {10},
pages = {1--13},
title = {{Drag force on a sediment particle from point velocity measurements: A spectral approach}},
volume = {46},
year = {2010}
}
