isbn = {0043-1370},
issn = {00431370},
journal = {Water Resources Bulletin},
keywords = {Aquatic environment,Bassin hydrographique,Biodiversity,Conservation,Diversit{\&}{\#}233,Drainage basins,Ecosyst{\&}{\#}232,Ecosystem,Milieu aquatique,Natural resources,Ressource naturelle,S{\&}{\#}233,Sedimentation,aquatic biodiversity,biologique,conservation reserves,cumulative effects,dimentation,ecological integrity,ecosystem management,landscape planning,me,watershed analysis},
number = {2},
