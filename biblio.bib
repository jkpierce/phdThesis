title = {{Interactions between bed forms: Topography, turbulence, and transport}},
volume = {110},
year = {2005}
}
@article{Hubbell1964,
abstract = {Since the advent of radioactive tracer techniques, application of$\backslash$nLagrangian experimental techniques in the investigation of sediment$\backslash$ntransport phenomena has become feasible. The results of one field$\backslash$nexperiment and two subsequent laboratory experiments in which radioactive$\backslash$ntracer techniques were used to study the transport and longitudinal$\backslash$ndispersion of bed-material particles are reported.$\backslash$n$\backslash$nThe development of a concentration distibution function, which is$\backslash$nbased on a Lagrangian probabilistic model wherein the transport of$\backslash$nbed material particles is described as a seuence of alternating steps$\backslash$nand rest periods, and a method for determining the discharge of bed-material$\backslash$nparticles are reviewed. the discharge is obtained from applying the$\backslash$ncontinuity principle to the mean rate of movement of a group of tracer$\backslash$nparticles and the effective cross-sectional area of the bed through$\backslash$nwhich they move. the concentration-distribution function and the$\backslash$nmethod for determining bed-material discharge are examined in the$\backslash$nlight of the experimental data and are found to be applicable both$\backslash$nin the field and the laboratory.},
