file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Papanicolaou et al. - 2002 - Stochastic incipient motion criterion for spheres under various bed packing conditions.pdf:pdf},
isbn = {0733-9429},
