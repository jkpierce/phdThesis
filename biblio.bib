file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Celik, Diplas, Dancey - 2014 - Instantaneous pressure measurements on a spherical grain under threshold flow conditions.pdf:pdf},
isbn = {9783939230007},
issn = {00221120},
