@article{Pacheco-Vazquez2017,
abstract = {We report on an experimental investigation of craters generated by explosions at the surface of a model granular bed. Following the initial blast, a pressure wave propagates through the bed, producing high-speed ejecta of grains and ultimately a crater. We analyzed the crater morphology in the context of large-scale explosions and other cratering processes. The process was analyzed in the context of large-scale explosions, and the crater morphology was compared with those resulting from other cratering processes in the same energy range. From this comparison, we deduce that craters formed through different mechanisms can exhibit fine surface features depending on their origin, at least at the laboratory scale. Moreover, unlike laboratory-scale craters produced by the impact of dense spheres, the diameter and depth do not follow a 1/4-power-law scaling with energy, rather the exponent observed herein is approximately 0.30, as has also been found in large-scale events. Regarding the ejecta curtain of grains, its expansion obeys the same time dependence followed by shock waves produced by underground explosions. Finally, from experiments in a two-dimensional system, the early cavity growth is analyzed and compared to a recent study on explosions at the surface of water.},
author = {Pacheco-V{\'{a}}zquez, F. and Tacum{\'{a}}, A. and Marston, J. O.},
doi = {10.1103/PhysRevE.96.032904},
file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Pacheco-V{\'{a}}zquez, Tacum{\'{a}}, Marston - 2017 - Craters produced by explosions in a granular medium.pdf:pdf},
issn = {24700053},
journal = {Physical Review E},
number = {3},
pages = {1--6},
pmid = {29346862},
title = {{Craters produced by explosions in a granular medium}},
volume = {96},
year = {2017}
}
@article{Martinez-Garcia2013,
abstract = {Regular vegetation patterns in semiarid ecosystems are believed to arise from the interplay between long-range competition and facilitation processes acting at smaller distances. We show that, under rather general conditions, long-range competition alone may be enough to shape these patterns. To this end we propose a simple, general model for the dynamics of vegetation, which includes only long-range competition between plants. Competition is introduced through a nonlocal term, where the kernel function quantifies the intensity of the interaction. We recover the full spectrum of spatial structures typical of vegetation models that also account for facilitation in addition to competition. Key Points Spatial vegetation model only considering long-range competition for water. The model does not consider short-range facilitation. Patterns in the system are determined by the kernel competition function. {\textcopyright}2013. American Geophysical Union. All Rights Reserved.},
author = {Mart{\'{i}}nez-Garc{\'{i}}a, Ricardo and Calabrese, Justin M. and Hern{\'{a}}ndez-Garc{\'{i}}a, Emilio and L{\~{o}}pez, Crist{\~{o}}bal},
