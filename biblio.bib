@article{Bradley2010,
abstract = {Random walk models of fluvial bed load transport use probability distributions to describe the distance a grain travels during$\backslash$n an episode of transport and the time it rests after deposition. These models typically employ probability distributions with$\backslash$n finite first and second moments, reflecting an underlying assumption that all the factors that influence sediment transport$\backslash$n tend to combine in such a way that the length of a step or the duration of a rest can be characterized by a mean value surrounded$\backslash$n by a specific amount of variability. The observation that many transport systems exhibit apparent scale-dependent behavior$\backslash$n and non-Fickian dispersion suggests that this assumption is not always valid. We revisit a nearly 50 year old tracer experiment$\backslash$n in which the tracer plume exhibits the hallmarks of dispersive transport described by a step length distribution with a divergent$\backslash$n second moment and no characteristic dispersive size. The governing equation of this type of random walk contains fractional-order$\backslash$n derivatives. We use the data from the experiment to test two versions of a fractional-order model of dispersive fluvial bed$\backslash$n load transport. The first version uses a heavy-tailed particle step length distribution with a divergent second moment to$\backslash$n reproduce the anomalously high fraction of tracer mass observed in the downstream tail of the spatial distribution. The second$\backslash$n version adds a feature that partitions mass into a detectable mobile phase and an undetectable, immobile phase. This two-phase$\backslash$n transport model predicts other features observed in the data: a decrease in the amount of detected tracer mass over the course$\backslash$n of the experiment and enhanced particle retention near the source. The fractional-order models match the observed plume shape$\backslash$n and growth rates better than prior attempts with classical models.},
author = {Bradley, D. Nathan and Tucker, Gregory E. and Benson, David A.},
doi = {10.1029/2009jf001268},
file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bradley, Tucker, Benson - 2010 - Fractional dispersion in a sand bed river.pdf:pdf},
isbn = {0148-0227},
issn = {0148-0227},
journal = {Journal of Geophysical Research},
keywords = {doi:10.102,http://dx.doi.org/10.1029/2009JF001268},
pages = {F00A09},
title = {{Fractional dispersion in a sand bed river}},
url = {http://doi.wiley.com/10.1029/2009JF001268},
volume = {115},
year = {2010}
}
