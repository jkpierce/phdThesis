file = {:home/kp/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Mart{\'{i}}nez-Garc{\'{i}}a et al. - 2013 - Vegetation pattern formation in semiarid systems without facilitative mechanisms.pdf:pdf},
